<Values version="2.0">
  <value name="xml">CREATE TABLE [dbo].[%logger%] (
	[ID] [bigint] IDENTITY (1, 1) NOT NULL ,
	[Time] [datetime] NOT NULL ,
	[Type] [varchar] (500) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
	[Level] [bigint] NOT NULL ,
	[Data] [varchar] (2000) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
	[XML] [varchar] (8000) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
	[SessionID] [varchar] (100) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
) ON [PRIMARY];
CREATE TABLE [dbo].[%logger%_attr] (
	[Name] [varchar] (500) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL ,
	[Description] [varchar] (500) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL 
) ON [PRIMARY];
CREATE TABLE [dbo].[%logger%_link] (
	[attr_name] [varchar] (500) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL,
	[log_ID] [bigint] NOT NULL ,
	[attr_value] [varchar] (500) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL
) ON [PRIMARY];
CREATE CLUSTERED  INDEX [IX_%logger%] ON [dbo].[%logger%]([Time]) ON [PRIMARY];
CREATE UNIQUE CLUSTERED  INDEX [IX_%logger%_attr] ON [dbo].[%logger%_attr]([NAME]) ON [PRIMARY];
CREATE UNIQUE INDEX [IX_%logger%_link] ON [dbo].[%logger%_link] ([attr_name],[attr_value],[log_ID] ) ON [PRIMARY];
ALTER TABLE [dbo].[%logger%] ADD 
	CONSTRAINT [DF_%logger%_Time] DEFAULT (getdate()) FOR [Time],
	CONSTRAINT [PK_%logger%] PRIMARY KEY  NONCLUSTERED 
	(
		[ID]
	)  ON [PRIMARY] ;
ALTER TABLE [dbo].[%logger%_attr] ADD 
	CONSTRAINT [PK_%logger%_attr] PRIMARY KEY  NONCLUSTERED 
	(
		[Name]
	)  ON [PRIMARY] ;
ALTER TABLE [dbo].[%logger%] ADD 
	CONSTRAINT [FK_%logger%_levels] FOREIGN KEY 
	(
		[Level]
	) REFERENCES [dbo].[levels] (
		[ID]
	);
ALTER TABLE [dbo].[%logger%_link] ADD 
	CONSTRAINT [FK_%logger%_link_%logger%] FOREIGN KEY 
	(
		[log_ID]
	) REFERENCES [dbo].[%logger%] (
		[ID]
	),
	CONSTRAINT [FK_%logger%_link_%logger%_attr] FOREIGN KEY 
	(
		[attr_name]
	) REFERENCES [dbo].[%logger%_attr] (
		[Name]
	),
	CONSTRAINT [PK_%logger%_link] PRIMARY KEY  NONCLUSTERED 
	(
		[log_ID],[attr_name],[attr_value]
	)  ON [PRIMARY] ;
;</value>
